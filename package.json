{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint --fix && eslint .",
    "lint-staged": "lint-staged  -q -p false -v > /dev/null && ( echo  \"\\033[0;32m\" && echo 'Staged files checks passed & has no errors'  && echo  \"\\033[0m\") || (echo  \"\\033[0;31m\" && echo 'Staged files have above Errors' && echo  \"\\033[0m\") ",
    "format:fix": " echo  \"\\033[0;32m\" && echo 'Prettier formating files' && echo  \"\\033[0m\" &&  prettier --write --ignore-path .gitignore . && echo  \"\\033[0;32m\" && echo 'Prettier fixed all files' && echo  \"\\033[0m\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,html}": [
      "npm run format:fix",
      "npm run eslint"
    ],
    "*.{css,json}": [
      "prettier --write --ignore-path .gitignore ."
    ]
  },
  "dependencies": {
    "gsap": "^3.12.2",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/prompt": "^18.4.3",
    "autoprefixer": "^10",
    "chalk": "^5.3.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3"
  }
}
